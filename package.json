		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test": "bun run test:unit && bun run test:e2e",
		"test:unit": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test"
	},
