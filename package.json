	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "bun run format:check && bun run lint:check && bun run svelte:check",
